{
  "name": "iedcwebsite",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.*\\.(css|js))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.*\\.(png|jpe?g|gif|svg|ico|webp))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.*\\.(woff|woff2|eot|ttf|otf))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/(.*\\.(css|js))$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*\\.(png|jpe?g|gif|svg|ico|webp))$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*\\.(woff|woff2|eot|ttf|otf))$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/index.html"
    }
  ]
}
